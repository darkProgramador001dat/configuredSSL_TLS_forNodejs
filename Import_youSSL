const path = require('path');
const fs = require('fs');


const privateKey = fs.readFileSync(path.join(__dirname, 'privatete_key.pem'), 'utf-8');
const certificate = fs.readFileSync(path.join(__dirname, 'certificate.csr'), 'utf-8');
const domain = [{ key: privateKey, cert: certificate }];









module.exports = domain;
